<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: lightblue;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.courseregistration.StudentScene">
   <children>
      <Button fx:id="quitButton" layoutX="491.0" layoutY="322.0" mnemonicParsing="false" onAction="#quit" text="Quit" />
      <Button fx:id="viewEnrButton" layoutX="219.0" layoutY="188.0" mnemonicParsing="false" onAction="#seeEnrolledCourses" text="View Enrolled Courses" />
      <Button fx:id="histButton" layoutX="219.0" layoutY="234.0" mnemonicParsing="false" onAction="#seeCourseHistory" prefWidth="163.0" text="View Course History" />
      <Button fx:id="addCourses" layoutX="219.0" layoutY="278.0" mnemonicParsing="false" onAction="#addCourses" prefWidth="163.0" text="Add Courses" />
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <Text fx:id="studentText" layoutX="61.0" layoutY="74.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Student: " />
      <Text fx:id="studentIDText" layoutX="62.0" layoutY="101.0" strokeType="OUTSIDE" strokeWidth="0.0" text="User_ID: " />
      <Text fx:id="counselorText" layoutX="62.0" layoutY="128.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Counselor:" />
      <Text fx:id="counselorIDText" layoutX="62.0" layoutY="154.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Counselor_ID:" />
      <Button fx:id="removeCourses" layoutX="219.0" layoutY="322.0" mnemonicParsing="false" onAction="#removeCourses" prefWidth="163.0" text="Remove Courses" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" onDragDetected="#goBack" text="Back" />
   </children>
</AnchorPane>
